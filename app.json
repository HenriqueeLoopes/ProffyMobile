{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.henriquelopes.proffy"
    },
    "android": {
      "package": "com.henriquelopes.proffy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
